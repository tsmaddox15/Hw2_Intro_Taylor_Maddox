<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".analogView">

    <com.example.tsmad_000.hw2_intro_taylor_maddox.CanvasClock
        android:id="@+id/aClock"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginStart="149dp"
        android:layout_marginTop="437dp" />

    <!--<AnalogClock-->
    <!--android:id="@+id/aClock"-->
    <!--android:layout_width="wrap_content"-->
    <!--android:layout_height="wrap_content"-->
    <!--android:layout_alignParentStart="true"-->
    <!--android:layout_alignParentTop="true"-->
    <!--android:layout_marginStart="149dp"-->
    <!--android:layout_marginTop="437dp" />-->

    <Button
        android:id="@+id/hourM1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="86dp"
        android:layout_marginTop="98dp"
        android:onClick="changeTime"
        android:text="-1 Hr" />

    <TextView
        android:id="@+id/changeH"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="77dp"
        android:text="Change Hour:" />

    <Button
        android:id="@+id/hourP1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/hourM1"
        android:layout_alignParentStart="true"
        android:onClick="changeTime"
        android:text="+1 HR" />

    <TextView
        android:id="@+id/ChaneMin1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="13dp"
        android:layout_marginTop="168dp"
        android:text="Change Minute:" />

    <Button
        android:id="@+id/minuteP1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/ChaneMin1"
        android:layout_alignParentStart="true"
        android:layout_marginStart="1dp"
        android:layout_marginTop="4dp"
        android:onClick="changeTime"
        android:text="+1 Min" />

    <Button
        android:id="@+id/minuteM1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/minuteP1"
        android:layout_marginStart="10dp"
        android:layout_marginBottom="1dp"
        android:layout_toEndOf="@+id/hourP1"
        android:onClick="changeTime"
        android:text="-1 Min" />

    <TextView
        android:id="@+id/changeSec1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="230dp"
        android:layout_marginTop="77dp"
        android:text="Chaneg Second:" />

    <Button
        android:id="@+id/newD1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="64dp"
        android:layout_marginTop="12dp"
        android:onClick="newView"
        android:text="New Digital" />

    <Button
        android:id="@+id/newA1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/changeSec1"
        android:layout_alignTop="@+id/newD1"
        android:onClick="newView"
        android:text="New Analog" />

    <Button
        android:id="@+id/secPlus1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/hourM1"
        android:layout_marginStart="6dp"
        android:layout_marginTop="0dp"
        android:layout_toEndOf="@+id/minuteM1"
        android:onClick="changeTime"
        android:text="+5 Sec" />

    <Button
        android:id="@+id/secM1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/hourM1"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="0dp"
        android:layout_marginEnd="12dp"
        android:onClick="changeTime"
        android:text="-5 Sec" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_centerVertical="true"
        android:text="Change Month" />

    <Button
        android:id="@+id/monthP1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView5"
        android:layout_alignParentStart="true"
        android:onClick="changeTime"
        android:text="Button" />

    <Button
        android:id="@+id/monthM1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/monthP1"
        android:layout_toEndOf="@+id/minuteP1"
        android:onClick="changeTime"

        android:text="Button" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/ChaneMin1"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="77dp"
        android:layout_marginBottom="0dp"
        android:text="Change Day" />

    <Button
        android:id="@+id/dayPlus1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/minuteM1"
        android:layout_toStartOf="@+id/secM1"
        android:onClick="changeTime"
        android:text="+1 Day" />

    <Button
        android:id="@+id/dayMinus1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/ChaneMin1"
        android:layout_alignStart="@+id/secM1"
        android:layout_marginStart="8dp"
        android:layout_marginTop="3dp"
        android:onClick="changeTime"
        android:text="-1 Day" />

    <Button
        android:id="@+id/undo1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/monthP1"
        android:layout_marginEnd="-95dp"
        android:layout_marginBottom="1dp"
        android:layout_toStartOf="@+id/secM1"
        android:onClick="changeTime"
        android:text="undo" />

    <Button
        android:id="@+id/redo1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/undo1"
        android:layout_toEndOf="@+id/minuteM1"
        android:onClick="changeTime"
        android:text="Redo" />

</RelativeLayout>